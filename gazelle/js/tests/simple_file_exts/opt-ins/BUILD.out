js_library(
    name = "opt-ins",
    srcs = [
        "non-typed.js",
        "non-typed.ts.js",
        "non-x-typed.jsx",
    ],
)

js_library(
    name = "opt-ins_tests",
    testonly = True,
    srcs = [
        "non-typed.spec.js",
        "non-x-typed.spec.jsx",
    ],
    deps = [":opt-ins"],
)

js_library(
    name = "m",
    srcs = ["non-typed.mjs"],
    deps = [":opt-ins"],
)

js_library(
    name = "c",
    srcs = ["non-typed.cjs"],
)

js_library(
    name = "mtest",
    testonly = True,
    srcs = ["non-typed.spec.mjs"],
    deps = [":m"],
)

js_library(
    name = "ctest",
    testonly = True,
    srcs = ["non-typed.spec.cjs"],
    deps = [":c"],
)
